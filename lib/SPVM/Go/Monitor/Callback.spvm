class Go::Monitor::Callback {
  use Go::Machine;
  
  static method new : Go::Monitor::Callback () {
    
    my $self = new Go::Monitor::Callback;
    
    return $self;
  }
  
  method : void () {
    
    while (1) {
      my $machine = $self->{machine};
      
      unless ($machine) {
        next;
      }
      
      
    }
  }
  
  has machine : rw Go::Machine;
}

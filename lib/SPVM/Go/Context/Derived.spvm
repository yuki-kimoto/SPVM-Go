class Go::Context::Derived {
  version_from Go;
  
  allow Go::Context;
  
  use Go::Context;
  use Go::Context::CancelCallback;
  
  # Fields
  has ctx : rw Go::Context;
  
  has cancel : rw Go::Context::CancelCallback;
  
  # Class Methods
  static method new : Go::Context::Derived () {
    
    my $self = new Go::Context::Derived;
    
    return $self;
  }
  
}

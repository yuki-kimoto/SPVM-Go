class Go::Context {
  version_from Go;
  
  use Go::Channel;
  use Go::Deadline;
  
  has deadline : ro Go::Deadline;
  
  has done : ro Go::Channel;
  
  # Instance Methods
  method err : string () { die "Not implemented."; }
  
  method value : object ($key : string) { die "Not implemented."; }
  
}

=pod

type Context interface {
    Deadline() (deadline time.Time, ok bool)
    Done() <-chan struct{}
    Err() error
    Value(key interface{}) interface{}
}

=cut

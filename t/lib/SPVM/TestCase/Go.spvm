      if ($coroutines->[1]->exception) {
        die $coroutines->[1]->exception;
      }
      
      warn("G0-B");
      
      Go::Coroutine->transfer($coroutines->[0], $coroutines->[1]);
      
      if ($coroutines->[1]->exception) {
        die $coroutines->[1]->exception;
      }
      
      warn("G0-C");
      

class TestCase::Go {
  use Go;
  
  use Go::Schedule;
  use Go::Machine;
  use Go::Process::Task;
  use Go::Coroutine;
  
  static method test_schedule : int () {
    
    my $schedule = Go::Schedule->new;
    
    my $machine = Go::Machine->new;
    
    my $process = (Go::Process)$schedule->process_queue->first;
    
    $process->set_schedule($schedule);
    
    $machine->set_process($process);
    
    my $process_task_callback = Go::Process::Task->new;
    
    $process_task_callback->set_machine($machine);
    
    my $process_task_coroutine = Go::Coroutine->new($process_task_callback);
    
    $process->set_task_coroutine($process_task_coroutine);
    
    my $coroutine0 = (Go::Coroutine)undef;
    
    $coroutine0 = Go::Coroutine->new([has machine : Go::Machine = $machine] method : void () {
      
      my $machine = $self->{machine};
      
      my $process = $machine->process;
      
      my $shcedule = $process->schedule;
      
      my $process_task_coroutine = $process->task_coroutine;
      
      # die "Foo";
      
      warn("G1-A");
      
      warn("G1-CallMethodStart");
      TestCase::Go->foo();
      warn("G1-CallMethodEnd");
      
      my $current_coroutine = $machine->coroutine;
      $process->coroutine_queue->enqueue($current_coroutine);
      Go::Coroutine->transfer($current_coroutine, $process_task_coroutine);
      
      warn("G1-B");
    }, $process_task_coroutine);
    
    $process->coroutine_queue->enqueue($coroutine0);
    
    $process->start;
    
    if ($coroutine0->exception) {
      die $coroutine0->exception;
    }
    warn("END");
    
    return 1;
  }
  
